<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="ShoppingList">
        <property name="idShoppingList" type="int" />
        <property name="name" type="string" />
    </transfer>

    <transfer name="ShoppingListCollection">
        <property name="shoppingLists" type="ShoppingList[]" singular="shoppingList" />
    </transfer>

    <transfer name="ProductQuantityStorage">
        <property name="quantityMin" type="float"/>
        <property name="quantityMax" type="float"/>
        <property name="quantityInterval" type="float"/>
    </transfer>

</transfers>
