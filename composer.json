{
  "name": "spryker-shop/shopping-list-widget",
  "description": "ShoppingListWidget module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker-shop/shop-application": "^1.0.0",
    "spryker/customer": "^7.4.0",
    "spryker/kernel": "^3.16.0",
    "spryker/shopping-list": "^1.0.0",
    "spryker/symfony": "^3.1.0"
  },
  "require-dev": {
    "spryker-shop/customer-page": "*",
    "spryker/product-detail-page": "*",
    "spryker/silex": "*"
  },
  "suggest": {
    "spryker-shop/customer-page": "Widget plugins are available for this module.",
    "spryker/silex": "Use this module when using plugins that need Silex dependencies.",
    "spryker/product-detail-page": "To use ShoppingListWidget plugin, ShoppingListWidgetPluginInterface from product-detail-page v0.2.2 is needed"
  },
  "autoload": {
    "psr-4": {
      "SprykerShop\\": "src/SprykerShop/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
